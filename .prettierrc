{
  "semi": true,
  "experimentalTernaries": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [{"files": "*.svelte", "options": {"parser": "svelte"}}]
}
